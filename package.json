{
    "name": "@jahia/moonstone",
    "version": "0.4.0",
    "dependencies": {
        "@svgr/webpack": "^4.3.3",
        "clsx": "^1.0.4",
        "prop-types": "^15.7.2"
    },
    "peerDependencies": {
        "react": "^16.11.0"
    },
    "scripts": {
        "start": "yarn index-icons && start-storybook -p 6006",
        "test": "jest --ci --runInBand --coverage",
        "tdd": "jest --watch",
        "lint:js": "eslint --ext js,jsx,json .",
        "lint:scss": "stylelint './src/**/*.scss'",
        "lint:js:fix": "yarn lint:js --fix .",
        "lint:scss:fix": "yarn lint:scss --fix",
        "lint:fix": "yarn lint:js:fix && yarn lint:scss:fix",
        "index-icons": "create-index ./src/icons/asset -r -x svg && eslint --fix src/icons/asset/index.js",
        "build": "yarn index-icons && yarn build:storybook && yarn build:lib",
        "prebuild:lib": "rimraf dist/",
        "build:lib": "webpack --mode production",
        "build:lib:dev": "webpack --mode development",
        "build:storybook": "build-storybook",
        "deploy": "rsync -r ./storybook-static/ jahia@design.int.jahia.com:/var/www/vhosts/contenteditor.jahia.design/html/moonstone/",
        "auto": "auto",
        "auto-version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
        "auto-publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
        "auto-release": "auto changelog && yarn run auto-version && yarn run auto-publish && auto release"
    },
    "main": "dist/main.js",
    "module": "dist/main.js",
    "repository": "https://github.com/Jahia/moonstone.git",
    "sideEffects": false,
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,scss}": [
            "yarn lint:fix",
            "git add"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.2",
        "@babel/preset-react": "^7.0.0",
        "@jahia/eslint-config": "^1.0.0-beta.20190819140611",
        "@storybook/addon-a11y": "^5.2.8",
        "@storybook/addon-actions": "^5.2.5",
        "@storybook/addon-docs": "^5.2.5",
        "@storybook/addon-knobs": "^5.2.5",
        "@storybook/addon-links": "^5.2.5",
        "@storybook/addon-storysource": "^5.2.5",
        "@storybook/addon-viewport": "^5.2.5",
        "@storybook/addons": "^5.2.5",
        "@storybook/react": "^5.2.5",
        "@storybook/source-loader": "^5.2.5",
        "@svgr/webpack": "^4.3.3",
        "auto": "^7.12.3",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "component-test-utils-react": "^1.0.0-alpha.11",
        "create-index": "^2.5.0",
        "css-loader": "^3.2.0",
        "eslint": "5.16.0",
        "eslint-plugin-jest": "^22.16.0",
        "eslint-plugin-json": "^1.4.0",
        "eslint-plugin-react": "^7.14.3",
        "file-loader": "^4.2.0",
        "husky": "^3.0.9",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.9.0",
        "lint-staged": "^9.4.1",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.13.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "re-resizable": "^6.1.0",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-test-renderer": "^16.11.0",
        "rimraf": "^3.0.0",
        "sass-loader": "^8.0.0",
        "storybook-addon-versions": "^0.4.3",
        "storybook-readme": "^5.0.8",
        "style-loader": "^1.0.0",
        "stylelint": "^11.1.1",
        "stylelint-config-css-modules": "^1.5.0",
        "stylelint-config-standard": "^19.0.0",
        "stylelint-order": "^3.1.0",
        "stylelint-scss": "^3.12.0",
        "terser-webpack-plugin": "^2.2.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-node-externals": "^1.7.2"
    }
}

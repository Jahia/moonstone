{
    "name": "@jahia/moonstone",
    "version": "2.3.3",
    "main": "dist/lib/main.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "@babel/runtime": "^7.12.5",
        "clsx": "^1.1.1",
        "prop-types": "^15.7.2",
        "re-resizable": "^6.9.1",
        "react-is": "^17.0.2",
        "to-px": "^1.1.0"
    },
    "peerDependencies": {
        "react": "^16.11.0",
        "react-table": "^7.7.0"
    },
    "scripts": {
        "start": "start-storybook -p 6006",
        "test": "jest --ci --runInBand --coverage",
        "tdd": "jest --watch",
        "lint:js": "eslint --ext js,jsx,json .",
        "lint:ts": "tslint 'src/**/*.ts?(x)'",
        "lint:scss": "stylelint './src/**/*.scss'",
        "lint": "yarn lint:js && yarn lint:ts && yarn lint:scss",
        "lint:js:fix": "yarn lint:js --fix .",
        "lint:ts:fix": "tslint 'src/**/*.ts?(x)' --fix",
        "lint:scss:fix": "yarn lint:scss --fix",
        "lint:fix": "yarn lint:js:fix && yarn lint:ts:fix && yarn lint:scss:fix",
        "compile:js": "node ./scripts/compile.js",
        "compile:css": "node ./scripts/compile-css.js",
        "tsc": "tsc --build tsconfig.json",
        "build:icons": "rimraf src/icons/components/*.tsx && svgr --template ./src/icons/svgrTemplate.js --config-file ./src/icons/svgr.config.js ./src/icons/assets && tslint --fix ./src/icons/components/*",
        "build": "yarn build:storybook && yarn build:lib && yarn webpack",
        "prebuild:lib": "rimraf dist/",
        "build:lib": "yarn tsc && yarn compile:js && yarn compile:css",
        "build:storybook": "build-storybook",
        "auto": "auto",
        "auto-changelog": "auto changelog",
        "auto-version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
        "auto-publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
        "auto-release-only": "auto release",
        "auto-release": "auto changelog && yarn run auto-version && yarn run auto-publish && auto release",
        "prepare": "husky install"
    },
    "repository": "https://github.com/Jahia/moonstone.git",
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,scss}": [
            "yarn lint:fix"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@jahia/eslint-config": "^1.1.0",
        "@jahia/stylelint-config": "^0.0.3",
        "@storybook/addon-a11y": "^6.4.5",
        "@storybook/addon-actions": "^6.4.5",
        "@storybook/addon-controls": "^6.4.5",
        "@storybook/addon-docs": "^6.4.5",
        "@storybook/addon-knobs": "^6.4.0",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-viewport": "^6.4.5",
        "@storybook/addons": "^6.4.5",
        "@storybook/builder-webpack5": "^6.4.5",
        "@storybook/manager-webpack5": "^6.4.5",
        "@storybook/react": "^6.4.5",
        "@storybook/source-loader": "^6.4.5",
        "@svgr/cli": "^6.1.0",
        "@svgr/core": "^6.1.0",
        "@testing-library/dom": "^8.11.1",
        "@testing-library/jest-dom": "^5.15.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/react": "17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-table": "^7.7.8",
        "@types/to-px": "^1.1.2",
        "auto": "^10.32.3",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.4.2",
        "babel-loader": "^8.2.3",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-transform-rename-import": "^2.3.0",
        "babel-plugin-typescript-to-proptypes": "^2.0.0",
        "component-test-utils-react": "^1.0.1-beta.2",
        "css-loader": "^6.5.1",
        "eslint": "^7.15.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "husky": "^7.0.4",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.4.3",
        "lint-staged": "^12.1.2",
        "mini-css-extract-plugin": "^2.4.5",
        "mkdir-recursive": "^0.4.0",
        "optimize-css-assets-webpack-plugin": "^6.0.1",
        "postcss": "^8.4.4",
        "postcss-loader": "^6.2.1",
        "postcss-preset-env": "^7.0.1",
        "react": "^17.0.2",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^17.0.2",
        "react-table": "^7.7.0",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.44.0",
        "sass-loader": "^12.3.0",
        "style-loader": "^3.3.1",
        "stylelint": "^13.8.0",
        "terser-webpack-plugin": "^5.2.5",
        "ts-loader": "^9.2.6",
        "tslint": "^6.1.2",
        "tslint-react": "^5.0.0",
        "typescript": "^4.5.2",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1",
        "webpack-node-externals": "^3.0.0"
    }
}

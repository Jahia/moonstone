{
    "name": "@jahia/moonstone",
    "version": "1.6.2",
    "main": "dist/lib/main.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "@babel/runtime": "^7.12.5",
        "clsx": "^1.1.1",
        "prop-types": "^15.7.2",
        "re-resizable": "^6.9.0",
        "react-is": "^17.0.1",
        "to-px": "^1.1.0"
    },
    "peerDependencies": {
        "react": "^16.11.0",
        "react-table": "^7.7.0"
    },
    "scripts": {
        "start": "start-storybook -p 6006",
        "test": "jest --ci --runInBand --coverage",
        "tdd": "jest --watch",
        "lint:js": "eslint --ext js,jsx,json .",
        "lint:ts": "tslint 'src/**/*.ts?(x)'",
        "lint:scss": "stylelint './src/**/*.scss'",
        "lint": "yarn lint:js && yarn lint:ts && yarn lint:scss",
        "lint:js:fix": "yarn lint:js --fix .",
        "lint:ts:fix": "tslint 'src/**/*.ts?(x)' --fix",
        "lint:scss:fix": "yarn lint:scss --fix",
        "lint:fix": "yarn lint:js:fix && yarn lint:ts:fix && yarn lint:scss:fix",
        "compile:js": "node ./scripts/compile.js",
        "compile:css": "node ./scripts/compile-css.js",
        "tsc": "tsc --build tsconfig.json",
        "prebuild-icons": "node ./scripts/rename.js ./src/icons/index.ts ./src/icons/tmp.js",
        "build-icons": "rimraf src/icons/*.tsx && svgr --typescript --template ./src/tokens/icons/svgrTemplate.js --config-file ./src/tokens/icons/svgr.config.json ./src/tokens/icons/assets --out-dir ./src/icons && tslint --fix ./src/icons",
        "postbuild-icons": "node ./scripts/rename.js ./src/icons/index.tsx ./src/icons/assets.ts && node ./scripts/rename.js ./src/icons/tmp.js ./src/icons/index.ts && eslint --fix ./src/icons/assets.ts",
        "build": "yarn build:storybook && yarn build:lib && yarn webpack",
        "prebuild:lib": "rimraf dist/",
        "build:lib": "yarn tsc && yarn compile:js && yarn compile:css",
        "build:storybook": "build-storybook",
        "auto": "auto",
        "auto-changelog": "auto changelog",
        "auto-version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
        "auto-publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
        "auto-release-only": "auto release",
        "auto-release": "auto changelog && yarn run auto-version && yarn run auto-publish && auto release",
        "prepare": "husky install"
    },
    "repository": "https://github.com/Jahia/moonstone.git",
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,scss}": [
            "yarn lint:fix"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@jahia/eslint-config": "^1.1.0",
        "@jahia/stylelint-config": "^0.0.3",
        "@storybook/addon-a11y": "^6.1.21",
        "@storybook/addon-actions": "^6.1.21",
        "@storybook/addon-controls": "^6.1.21",
        "@storybook/addon-docs": "^6.1.21",
        "@storybook/addon-knobs": "^6.1.21",
        "@storybook/addon-storysource": "^6.1.21",
        "@storybook/addon-viewport": "^6.1.21",
        "@storybook/addons": "^6.1.21",
        "@storybook/react": "^6.1.21",
        "@storybook/source-loader": "^6.1.21",
        "@svgr/cli": "^5.5.0",
        "@svgr/core": "^5.5.0",
        "@testing-library/dom": "^7.28.1",
        "@testing-library/jest-dom": "^5.11.6",
        "@testing-library/react": "^11.2.2",
        "@testing-library/user-event": "^12.5.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-table": "^7.0.29",
        "@types/to-px": "^1.1.0",
        "auto": "^10.4.2",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-transform-rename-import": "^2.3.0",
        "babel-plugin-typescript-to-proptypes": "^1.4.2",
        "component-test-utils-react": "^1.0.1-beta.2",
        "css-loader": "^5.0.1",
        "eslint": "^7.15.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "husky": "^7.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^11.1.0",
        "mini-css-extract-plugin": "^1.3.2",
        "mkdir-recursive": "^0.4.0",
        "node-sass": "^5.0.0",
        "node-sass-json-importer": "^4.3.0",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "postcss": "^8.2.15",
        "postcss-loader": "^4.2.0",
        "postcss-preset-env": "^6.7.0",
        "react": "^17.0.1",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^17.0.1",
        "react-table": "^7.7.0",
        "react-test-renderer": "^17.0.1",
        "rimraf": "^3.0.2",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "stylelint": "^13.8.0",
        "terser-webpack-plugin": "^5.0.3",
        "ts-loader": "^8.0.11",
        "tslint": "^6.1.2",
        "tslint-react": "^5.0.0",
        "typescript": "^4.1.2",
        "webpack": "^5.9.0",
        "webpack-cli": "^4.2.0",
        "webpack-node-externals": "^2.5.2"
    }
}

{
  "dimensions": false,
  "typescript": true,
  "outDir": "src/icons",
  "prettierConfig": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "svgoConfig": {
    "plugins": [
      {
        "name": "removeAttrs",
        "params": {
          "attrs": "fill"
        }
      },
      { "name": "addAttributesToSVGElement",
        "params": {
          "attribute": {
            "fill": "currentColor"
            }
          }
      },
      {
        "name": "removeDimensions"
      }
    ]
  }
}
